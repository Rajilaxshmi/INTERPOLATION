clc;
close all;
clear all;
N=input('enter the sequence 1 length N:');
M=input('enter the upsampling factor M:');
f1=0.01;
f2=0.2;
t=0:1:N-1;
x=sin(2*pi*f1*t)+sin(2*pi*f2*t);
x1=[zeros(1,M*N);
t1=1:1:M*N;
j=1:M:M*N;
x1(j)=x;
subplot(3,1,1);
plot(t,x);
xlabel('time');
ylabel('amplitude');
title('analog signal');
subplot(3,1,2);
stem(t,x);
xlabel('n');
ylabel('amplitude');
title('sampled signal');
subplot(3,1,3);
stem(t1-1,x1);
xlabel('n');
ylabel('amplitude');
title('upsampled signal');
